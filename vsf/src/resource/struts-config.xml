<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd"><struts-config>	<data-sources />	<form-beans>	</form-beans>	<global-exceptions />	<global-forwards />	<action-mappings>		<action path="/demo"			type="org.springframework.web.struts.DelegatingActionProxy" >			<forward name="success" path="/demo/page/demo.jsp"></forward>		</action>		<action path="/login"			type="org.springframework.web.struts.DelegatingActionProxy" >			<forward name="Login.Success" path="/la/pages/login/MyJsp.jsp"/>			<forward name="Login.Error" path="/la/pages/login/MyJsp1.jsp"/>		</action>	</action-mappings>	<plug-in className="org.springframework.web.struts.ContextLoaderPlugIn">  	    <set-property property="contextClass" value="org.springframework.web.context.support.XmlWebApplicationContext"/>  	    <set-property property="contextConfigLocation" value="/WEB-INF/classes/actionContext.xml,/WEB-INF/classes/serverContext.xml,/WEB-INF/classes/databaseContext.xml,/WEB-INF/classes/daoContext.xml,/WEB-INF/classes/aop.xml"/>  	</plug-in></struts-config>