<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">	<!-- Servers -->	<bean name="serverLocator" class="com.vesmile.webappdemo.server.ServerLocator">		<property name="demoServer" ref="demoServer" />		<property name="baseServer" ref="baseServer" />	</bean>		<bean name="baseServer" class="com.vesmile.webappdemo.server.impl.DemoBaseServer">		<property name="daoFacade" ref="daoFacade" />		<property name="baseDao" ref="baseDao" />	</bean>		<bean name="demoServer" class="com.vesmile.webappdemo.server.impl.DemoServerImpl" parent="baseServer">	</bean>	</beans>