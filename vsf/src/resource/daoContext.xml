<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">	<!-- Daos -->	<bean name="daoFacade" class="com.vesmile.webappdemo.dao.DaoFacade">		<property name="demoDao" ref="demoDao"></property>	</bean>		<bean name="baseDao" class="com.vesmile.framework.dao.BaseDao">		<property name="h3Support" ref="hibernateSupport" />		<property name="jdbcSupport" ref="jdbcSupport" />		<property name="iBatisSupport" ref="ibatisSupport" />	</bean>			<bean name="demoDao" class="com.vesmile.webappdemo.dao.impl.DemoDaoImpl" parent="baseDao" />	</beans>