<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">	<!-- Daos -->	<bean name="daoFacade" class="com.vesmile.webappla.dao.DaoFacade">		<property name="baseDao" ref="baseDao" />		<property name="userDao" ref="userDao" />		<property name="menuDao" ref="menuDao" />	</bean>		<bean name="baseDao" class="com.vesmile.framework.dao.BaseDao">		<property name="h3Support" ref="hibernateSupport" />		<property name="jdbcSupport" ref="jdbcSupport" />		<property name="iBatisSupport" ref="ibatisSupport" />		<property name="myBatisSupport" ref="myBatisSupport" />	</bean>		<bean name="userDao" class="com.vesmile.webappla.dao.UserDao" parent="baseDao" />	<bean name="menuDao" class="com.vesmile.webappla.dao.MenuDao" parent="baseDao" />		</beans>